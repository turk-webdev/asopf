<div class="text-center pt-5">
    <div class="card mx-auto" style="width: 48rem;">
        <div class="card-header">
            Covid Data By County
        </div>
        <div class="card-body">
            <form>
                <div class="form-group row">
                    <label for="inputCounty" class="col-sm-2 col-form-label">County</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" id="inputCounty" placeholder="County">
                    </div>
                </div>
                <div class="form-group row">
                    <label for="inputNewCases" class="col-sm-2 col-form-label">New Cases</label>
                    <div class="col-sm-10">
                        <input type="number" class="form-control" id="inputNewCases">
                    </div>
                </div>
                <div class="form-group row">
                    <label for="inputNewDeaths" class="col-sm-2 col-form-label">New Deaths</label>
                    <div class="col-sm-10">
                        <input type="number" class="form-control" id="inputNewDeaths">
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-sm-10 offset-sm-1">
                        <button type="submit" class="btn btn-info">Submit</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>

